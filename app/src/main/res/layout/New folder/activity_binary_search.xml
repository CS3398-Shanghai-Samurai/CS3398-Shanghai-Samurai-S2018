<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".BinarySearch">

    <ImageButton
        android:id="@+id/imageButton12"
        android:layout_width="291dp"
        android:layout_height="43dp"
        android:layout_marginBottom="88dp"
        android:layout_marginTop="72dp"
        android:background="@string/hello_blank_fragment"
        android:onClick="goToCode"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.505"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView14"
        app:srcCompat="@drawable/code" />

    <ImageButton
        android:id="@+id/imageButton15"
        android:layout_width="291dp"
        android:layout_height="45dp"
        android:layout_marginEnd="44dp"
        android:layout_marginLeft="49dp"
        android:layout_marginRight="44dp"
        android:layout_marginStart="49dp"
        android:layout_marginTop="232dp"
        android:background="@string/hello_blank_fragment"
        android:onClick="goToE1"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toTopOf="@+id/imageButton9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/example" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="2dp"
        android:layout_marginEnd="195dp"
        android:layout_marginLeft="1dp"
        android:layout_marginRight="195dp"
        android:layout_marginStart="1dp"
        android:text="Binary Search"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/textView3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Binary Search searches a sorted array by repeatedly dividing the search interval in half. Begin with an interval covering the whole array. If the value of the search key is less than the item in the middle of the interval, narrow the interval to the lower half. Otherwise, narrow it to the upper half. This process repeats until the value is found or the interval is empty."
        android:textAllCaps="false"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="41dp" />

</android.support.constraint.ConstraintLayout>