<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".InsertionSort">

    <ImageButton
        android:id="@+id/imageButton13"
        android:layout_width="291dp"
        android:layout_height="45dp"
        android:layout_marginEnd="44dp"
        android:layout_marginLeft="49dp"
        android:layout_marginRight="44dp"
        android:layout_marginStart="49dp"
        android:layout_marginTop="312dp"
        android:background="@string/hello_blank_fragment"
        android:onClick="goToE2"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toTopOf="@+id/imageButton9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/example" />

    <ImageButton
        android:id="@+id/imageButton14"
        android:layout_width="291dp"
        android:layout_height="45dp"
        android:layout_marginEnd="44dp"
        android:layout_marginLeft="49dp"
        android:layout_marginRight="44dp"
        android:layout_marginStart="49dp"
        android:layout_marginTop="192dp"
        android:background="@string/hello_blank_fragment"
        android:onClick="goToE1"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toTopOf="@+id/imageButton9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/example" />

    <ImageButton
        android:id="@+id/imageButton10"
        android:layout_width="291dp"
        android:layout_height="43dp"
        android:layout_marginBottom="28dp"
        android:layout_marginTop="72dp"
        android:background="@string/hello_blank_fragment"
        android:onClick="goToCode"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.505"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView14"
        app:srcCompat="@drawable/code" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="2dp"
        android:layout_marginEnd="195dp"
        android:layout_marginLeft="1dp"
        android:layout_marginRight="195dp"
        android:layout_marginStart="1dp"
        android:layout_marginTop="4dp"
        android:text="Insertion Sort"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/textView3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="364dp"
        android:layout_height="106dp"
        android:text="Insertion sort is a simple sorting algorithm that works the way we sort playing cards in our hands. Click one of the examples below."
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="45dp" />

</android.support.constraint.ConstraintLayout>